{
  "Name": "Ins-1",
  "CreationDate": "2026-2-25",
  "Commands": [
    {
      "Command": "prompt",
      "Target": "Please paste the JSON data for the insurance policy:",
      "Value": "jsonDataString",
      "Description": "Prompt the user to paste the JSON data"
    },
    {
      "Command": "executeScript_Sandbox",
      "Target": "return JSON.parse(${jsonDataString});",
      "Value": "jsonData",
      "Description": "Parse the JSON string into an object"
    },
    {
      "Command": "type",
      "Target": "name=resdate",
      "Value": "${jsonData.reservationDate}",
      "Description": "Fill the Reservation Date field"
    },
    {
      "Command": "type",
      "Target": "id=vendorName",
      "Value": "${jsonData.vendorName}",
      "Description": "Fill the Vendor Name field"
    },
    {
      "Command": "type",
      "Target": "name=confirmationnumber",
      "Value": "${jsonData.confirmationNumber}",
      "Description": "Fill the Confirmation Number field"
    },
    {
      "Command": "type",
      "Target": "name=duration",
      "Value": "${jsonData.duration}",
      "Description": "Fill the Duration field"
    },
    {
      "Command": "type",
      "Target": "id=noofpax",
      "Value": "${jsonData.noofpax}",
      "Description": "Fill the Number of Passengers field"
    },
    {
      "Command": "type",
      "Target": "id=noofunits",
      "Value": "${jsonData.noofunits}",
      "Description": "Fill the Number of Units field"
    },
    {
      "Command": "select",
      "Target": "id=interLinkcode",
      "Value": "label=${jsonData.tripType}",
      "Description": "Select the Trip Type"
    },
    {
      "Command": "type",
      "Target": "id=totalbase",
      "Value": "${jsonData.totalBase}",
      "Description": "Fill the Total Base/Premium field"
    },
    {
      "Command": "type",
      "Target": "id=totalcommission",
      "Value": "${jsonData.totalCommission}",
      "Description": "Fill the Total Commission field"
    }
  ]
}