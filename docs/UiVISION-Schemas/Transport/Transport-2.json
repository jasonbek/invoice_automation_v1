{
  "Name": "Transport-2",
  "CreationDate": "2026-2-25",
  "Commands": [
    {
      "Command": "prompt",
      "Target": "Please paste your JSON data:",
      "Value": "jsonDataString",
      "Description": "Prompt the user to enter or paste the JSON data"
    },
    {
      "Command": "executeScript_Sandbox",
      "Target": "return JSON.parse(${jsonDataString});",
      "Value": "jsonData",
      "Description": "Parse the JSON string into an object"
    },
    {
      "Command": "type",
      "Target": "name=serviceprovidername",
      "Value": "${jsonData.serviceProviderName}",
      "Description": "Fill the Service Provider Name field"
    },
    {
      "Command": "type",
      "Target": "id=startDate",
      "Value": "${jsonData.startDate}",
      "Description": "Fill the Start Date field"
    },
    {
      "Command": "type",
      "Target": "id=endDate",
      "Value": "${jsonData.endDate}",
      "Description": "Fill the End Date field"
    },
    {
      "Command": "type",
      "Target": "name=category",
      "Value": "${jsonData.category}",
      "Description": "Fill the Category field usually this is the kind of vehicle provided "
    },
    {
      "Command": "type",
      "Target": "name=description",
      "Value": "${jsonData.description}",
      "Description": "Fill the Description field - a long description of the vehicle provided "
    },
    {
      "Command": "type",
      "Target": "name=starttime",
      "Value": "${jsonData.startTime}",
      "Description": "Fill the Start Time field"
    },
    {
      "Command": "type",
      "Target": "name=endtime",
      "Value": "${jsonData.endTime}",
      "Description": "Fill the End Time field"
    },
    {
      "Command": "type",
      "Target": "name=departcityname",
      "Value": "${jsonData.departureCity}",
      "Description": "Fill the exact pick up location and description "
    },
    {
      "Command": "type",
      "Target": "name=arrivecityname",
      "Value": "${jsonData.arrivalCity}",
      "Description": "Fill the exact drop off location and description"
    },
    {
      "Command": "type",
      "Target": "name=clientfeedback",
      "Value": "${jsonData.clientFeedback}",
      "Description": "Provide any notes and details to make sure the pick up and drop off is a success."
    },
    {
      "Command": "type",
      "Target": "name=agentremarks",
      "Value": "${jsonData.agentRemarks}",
      "Description": "Fill the Agent Remarks field"
    }
  ]
}