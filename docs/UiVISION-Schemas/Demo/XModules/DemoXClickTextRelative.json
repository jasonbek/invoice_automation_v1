    {
      "Command": "executeScript_Sandbox",
      "Target": "return parseInt(${s}) ;",
      "Value": "i",
      "Description": "Convert string to integer number (remove spaces, line breaks etc)"
    },
    {
      "Command": "if",
      "Target": "${i} == 64",
      "Value": "",
      "Description": ""
    },
    {
      "Command": "echo",
      "Target": "8 x 8 is ${i}, Calculator works!",
      "Value": "green",
      "Description": ""
    },
    {
      "Command": "else",
      "Target": "",
      "Value": "",
      "Description": ""
    },
    {
      "Command": "echo",
      "Target": "i = ${i}",
      "Value": "blue",
      "Description": ""
    },
    {
      "Command": "throwError",
      "Target": "Calculator result is wrong",
      "Value": "",
      "Description": "We trigger an error if the value is not 64 (this is useful for automated testing)"
    },
    {
      "Command": "end",
      "Target": "",
      "Value": "",
      "Description": ""
