{
  "Name": "Cruise-1",
  "CreationDate": "2026-2-25",
  "Commands": [
    {
      "Command": "prompt",
      "Target": "Please paste the JSON data for the cruise booking:",
      "Value": "jsonDataString",
      "Description": "Prompt user for JSON data string"
    },
    {
      "Command": "executeScript_Sandbox",
      "Target": "var cleanedString = ${jsonDataString}.replace(/\\u00A0/g, ' '); return JSON.parse(cleanedString)[0];",
      "Value": "jsonData",
      "Description": "Clean and parse the JSON array"
    },
    {
      "Command": "type",
      "Target": "name=resdate",
      "Value": "${jsonData.reservationDate}",
      "Description": "Enter Reservation Date"
    },
    {
      "Command": "type",
      "Target": "id=vendorName",
      "Value": "${jsonData.vendorName}",
      "Description": "Enter Vendor Name"
    },
    {
      "Command": "type",
      "Target": "name=confirmationnumber",
      "Value": "${jsonData.confirmationNumber}",
      "Description": "Enter Confirmation Number"
    },
    {
      "Command": "type",
      "Target": "name=duration",
      "Value": "${jsonData.duration}",
      "Description": "Enter Duration"
    },
    {
      "Command": "type",
      "Target": "id=noofpax",
      "Value": "${jsonData.noofpax}",
      "Description": "Enter Number of Passengers"
    },
    {
      "Command": "type",
      "Target": "id=noofunits",
      "Value": "${jsonData.noofunit}",
      "Description": "Enter Number of Units"
    },
    {
      "Command": "select",
      "Target": "id=interLinkcode",
      "Value": "label=${jsonData.tripType}",
      "Description": "Select Trip Type"
    },
    {
      "Command": "type",
      "Target": "id=totalbase",
      "Value": "${jsonData.totalBase}",
      "Description": "Enter Total Base Cost"
    },
    {
      "Command": "type",
      "Target": "id=totaltax",
      "Value": "${jsonData.totalTax}",
      "Description": "Enter Total Tax"
    },
    {
      "Command": "type",
      "Target": "id=totalcommission",
      "Value": "${jsonData.totalCommission}",
      "Description": "Enter Total Commission"
    },
    {
      "Command": "type",
      "Target": "name=finalpymntduedate",
      "Value": "${jsonData.finalpymntduedate}",
      "Description": "Enter Final Payment Due Date"
    },
    {
      "Command": "type",
      "Target": "name=invoiceremarks",
      "Value": "${jsonData.invoiceRemarks}",
      "Description": "Enter Invoice Remarks"
    },
    {
      "Command": "type",
      "Target": "name=agentremarks",
      "Value": "${jsonData.agentRemarks}",
      "Description": "Enter Agent Remarks"
    }
  ]
}