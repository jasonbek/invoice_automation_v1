{
  "Name": "Hotel-1",
  "CreationDate": "2026-2-25",
  "Commands": [
    {
      "Command": "prompt",
      "Target": "Please paste your JSON data:",
      "Value": "jsonDataString",
      "Description": "Prompt the user to enter or paste the JSON data"
    },
    {
      "Command": "executeScript_Sandbox",
      "Target": "return JSON.parse(${jsonDataString});",
      "Value": "jsonData",
      "Description": "Parse the JSON string into an object"
    },
    {
      "Command": "type",
      "Target": "name=resdate",
      "Value": "${jsonData.bookingDate}",
      "Description": "Fill the Reservation Date field"
    },
    {
      "Command": "type",
      "Target": "id=vendorName",
      "Value": "${jsonData.vendor}",
      "Description": "Fill the Vendor Name field"
    },
    {
      "Command": "type",
      "Target": "name=confirmationnumber",
      "Value": "${jsonData.confirmationNumber}",
      "Description": "Fill the Confirmation Number field"
    },
    {
      "Command": "type",
      "Target": "name=duration",
      "Value": "${jsonData.numberOfNights}",
      "Description": "Fill the Duration (nights) field"
    },
    {
      "Command": "type",
      "Target": "id=noofpax",
      "Value": "${jsonData.numberOfGuests}",
      "Description": "Fill the Number of Guests field"
    },
    {
      "Command": "type",
      "Target": "id=noofunits",
      "Value": "${jsonData.numberOfUnits}",
      "Description": "Fill the Number of Units field"
    },
    {
      "Command": "select",
      "Target": "id=interLinkcode",
      "Value": "label=${jsonData.category}",
      "Description": "Select the Category (e.g., International)"
    },
    {
      "Command": "type",
      "Target": "id=totalbase",
      "Value": "${jsonData.baseAmount}",
      "Description": "Fill the Total Base Amount field"
    },
    {
      "Command": "type",
      "Target": "id=totaltax",
      "Value": "${jsonData.taxAmount}",
      "Description": "Fill the Total Tax Amount field"
    },
    {
      "Command": "type",
      "Target": "id=totalcommission",
      "Value": "${jsonData.commissionAmount}",
      "Description": "Fill the Total Commission field"
    }
  ]
}