{
  "Name": "Day-Tours-1",
  "CreationDate": "2026-2-25",
  "Commands": [
    {
      "Command": "prompt",
      "Target": "Paste JSON for Seat Provider Details:",
      "Value": "daytourDetailsString",
      "Description": "Get JSON data"
    },
    {
      "Command": "executeScript_Sandbox",
      "Target": "var cleanedString = ${daytourDetailsString}.replace(/\\u00A0/g, ' '); return JSON.parse(cleanedString);",
      "Value": "jsonData",
      "Description": "Clean and parse JSON"
    },
    {
      "Command": "type",
      "Target": "name=resdate",
      "Value": "${jsonData.dateReserved}",
      "Description": "Fill the Date Reserved field"
    },
    {
      "Command": "type",
      "Target": "id=vendorName",
      "Value": "${jsonData.vendor}",
      "Description": "Fill the Vendor field"
    },
    {
      "Command": "type",
      "Target": "name=confirmationnumber",
      "Value": "${jsonData.confirmationNumber}",
      "Description": "Fill the Confirmation Number field"
    },
    {
      "Command": "type",
      "Target": "name=duration",
      "Value": "${jsonData.duration}",
      "Description": "Fill the Duration field"
    },
    {
      "Command": "type",
      "Target": "id=noofpax",
      "Value": "${jsonData.numberOfTravellers}",
      "Description": "Fill the No. of Pax field"
    },
    {
      "Command": "select",
      "Target": "id=interLinkcode",
      "Value": "label=${jsonData.tripType}",
      "Description": "Select the Trip Type (D/I/T)"
    },
    {
      "Command": "type",
      "Target": "id=totalbase",
      "Value": "${jsonData.basePrice}",
      "Description": "Fill the Base field"
    },
    {
      "Command": "type",
      "Target": "id=totalcommission",
      "Value": "${jsonData.commission}",
      "Description": "Fill the Commission field"
    },
    {
      "Command": "type",
      "Target": "name=finalpymntduedate",
      "Value": "${jsonData.finalPaymentDue}",
      "Description": "Fill the Final Payment Due Date field"
    },
    {
      "Command": "type",
      "Target": "name=invoiceremarks",
      "Value": "${jsonData.invoiceRemarks}",
      "Description": "Fill the Invoice Remarks field"
    },
    {
      "Command": "type",
      "Target": "name=agentremarks",
      "Value": "${jsonData.agentRemarks}",
      "Description": "Fill the Agent Remarks field"
    }
  ]
}